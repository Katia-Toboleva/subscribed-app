    "graphql": "^14.5.8",
    "graphql-scalars": "^1.17.0",
    "graphql-tag": "^2.10.1",
    "json5": "^2.1.0",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.5.0",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
